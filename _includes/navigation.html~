<ul class="nav nav-list">  
     {% assign collections = site.collections  | sort: "display-order"  %}
        {% for collection in collections %}
           <li class="nav-header">{{ collection.title | capitalize }}</li>
          
           {% assign documents = collection.docs  | sort: "order"  %}
           {% for document in documents %}
               {% assign pageUrlSplited = page.url | split: "/" %}
			      {% assign pageVersion = pageUrlSplited[2] %}
					{% for version in document.version %}               
               	{% if version == pageVersion %}
                       <li data-page-id="{{document.path}}" data-order="{{ document.order }}"><a href="{{ site.baseurl }}{{ document.url }}">{{ document.title }}</a></li>
						{% endif%}
					{% endfor %}
           {% endfor %}
        {% endfor %}
</ul>
